        <!-- MIST -->
        <!-- size slider -->
        <view control-tag="Unit1::ReverbSize" class="CKnob" angle-range="270" angle-start="135" autosize="right top"
              background-offset="0, 0" circle-drawing="true" corona-color="~ WhiteCColor" corona-dash-dot="false"
              corona-drawing="true" corona-from-center="false" corona-inset="5" corona-inverted="false"
              corona-line-cap-butt="false" corona-outline="true" corona-outline-width-add="2" default-value="0.5"
              handle-color="~ WhiteCColor" handle-line-width="5" handle-shadow-color="~ GreyCColor" max-value="1"
              min-value="0" mouse-enabled="true" opacity="1" origin="20, 105" size="60, 60"
              skip-handle-drawing="false" transparent="false" value-inset="5" wants-focus="true"
              wheel-inc-value="0.1" zoom-factor="1.5"
        />

        <!-- width slider -->
        <view control-tag="Unit1::ReverbWidth" class="CKnob" angle-range="270" angle-start="135" autosize="right top"
              background-offset="0, 0" circle-drawing="true" corona-color="~ WhiteCColor" corona-dash-dot="false"
