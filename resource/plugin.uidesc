              wheel-inc-value="0.1" zoom-factor="1.5"
        />

        <!-- freeze toggle -->
        <view control-tag="Unit1::ReverbFreeze" class="CCheckBox" origin="224, 124" size="25, 40" autosize-to-fit="false"
              background-offset="0, 0" boxfill-color="#85bad7"  autosize="bottom"
              boxframe-color="~ BlackCColor" checkmark-color="~ BlackCColor"
